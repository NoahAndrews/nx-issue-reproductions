{
    "$schema": "./node_modules/nx/schemas/nx-schema.json",
    "targetDefaults": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["{projectRoot}/build"],
            "cache": true
        },
        "typecheck": {
            "dependsOn": ["^typecheck"],
            "cache": true
        },
        "build-dev": {
            "dependsOn": ["^build-dev"],
            "outputs": ["{projectRoot}/build"],
            "cache": true
        }
    },
    "defaultBase": "main",
    "plugins": [

    ],
    "namedInputs": {
        "default": ["{projectRoot}/**/*", "sharedGlobals"],
        "sharedGlobals": [
            "{workspaceRoot}/tsconfig-base.json"
        ]
    }
}
